"UnLitGeneric"
{

	"$baseTexture" "models/player/aliendrone_v3/slow_alien_blood"
	"$model" 1
	"$halflambert" 1
	"$translucent" 1
	"$nocull" 1

	"$envmap" "env_cubemap"
